<form method="POST" action="/dogs/<%= dogId %>/?_method=PUT">
    <label for="dogName">Name</label>
    <input id="dogName" type="text" name="name" value="<%= dog.name %>">
    <label for="dogName">Image URL</label>
    <input id="dogURL" type="text" name="url" value="<%= dog.image %>">
    <label for="dogFame">Famous for</label>
    <input id="dogFame" type="text" name="fame" value="<%= dog.famousFor %>">
    <input type="submit">
</form>